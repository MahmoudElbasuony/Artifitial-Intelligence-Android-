<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eatbeat"
    android:installLocation="preferExternal"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            
            android:name="eatbeat.GameLevelActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            
            android:screenOrientation="portrait" 
            android:name="eatbeat.LoadingScreenActivity"
            android:label="@string/title_activity_mainactivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="eatbeat.AboutInfoActivity"
            android:label="@string/title_activity_aboutinfo" >
        </activity>
        <activity
            android:name="eatbeat.ChooseOptionActivity"
            android:label="@string/title_activity_chooseoption" >
        </activity>
        <activity
            android:name="eatbeat.StartGameActivity"
            android:label="@string/title_activity_sndlyot" >
        </activity>
        <activity

            android:name="eatbeat.PlayWithActivity"
            android:label="@string/title_activity_thrdlyt" >
        </activity>
        <activity
            android:name="eatbeat.LevelChooseActivity"
            android:label="Choose Level" >
        </activity>
        <activity
            android:name="eatbeat.RemotePlayActivity"
            android:label="Discover Player" >
        </activity>
        <activity
            android:name="eatbeat.ScoresActivity"
            android:label="@string/title_activity_scores" >
        </activity>
        <activity
            android:name="eatbeat.RemoteConnectLoadingActivity"
            android:label="@string/title_activity_remote__connect__load" >
        </activity>
    </application>

</manifest>
